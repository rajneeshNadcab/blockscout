<style>
.search_backdrop {  
  background-image: linear-gradient(to right, #746305, #756e15, #767a24, #778532, #779041);
  width: 100%;
  color: #fff;
}

.search_backdrop_overlay {
  background-image: url(/images/backdrop-bg.svg);
  background-repeat: no-repeat;
  background-position: left center;
}

.footer_backdrop {  
  background-image: linear-gradient(to right, #746305, #756e15, #767a24, #778532, #779041);
  width: 100%;
  color: #fff;
}

.footer_overlay {
  background-image: url(/images/backdrop-bg.svg);
  background-repeat: repeat;
  background-position: left center;
}
.btn-teal {
  color: #00c9a7;
  background: rgba(0,201,167,.1);
  border-color: transparent;
}

.navbar-expand-lg .navbar-nav .dropdown-menu {
  width: 200px;
}
.scroll_box .hide_for_homepage {
  display: none
}

.show_for_hompage:not(.scroll_box .show_for_hompage){
  display: none;
}

.join {
  background: rgb(149,97,221);
  background: linear-gradient(50deg, rgba(149,97,221,1) 0%, rgba(207,205,138,1) 77%, rgba(207,205,138,1) 100%);
  padding:2px 5px;
  color:#333;
  font-size:10px;
  border-radius: 5px;
  margin: 5px; 
  font-weight:bold 
}

.overflow-auto {
  overflow: auto;
}

.card-tab {
  color:#333!important;
  height:45px!important;
}

.card-tab.active {
  color: #a9ab3f!important;
  border-bottom: 2px solid #a9ab3f;
  background:transparent!important;  
}

.dropdown-item {
  color: #333!important;
  font-size: 13px!important;
}

@media only screen and (min-device-width: 240px) and (max-device-width: 991px) {
  .width_full {
    width: 100%;
  }

  .navbar-expand-lg .navbar-nav .dropdown-menu {
    width: 100%;
    box-shadow: none;
  }
}

@media only screen and (min-device-width: 991px){
  .navbar .dropdown-menu {
    width: 250px;
    border-top: 3px solid #a9ab3f !important;
}
.header-nav-separator {
  border-left: 1px solid #e7eaf3;
  width: 1px;
  height: 20px;
} 

}

@media screen and (max-device-width:991px){
  .custom_navbar .dropdown-toggle::after {
    display:none
   }

   .navbar .dropdown-menu {
    border-left: 3px solid #a9ab3f;
    border-top:none!important
  }

}
@media screen and (max-device-width: 480px) {

  .hide_on_xs {
    display:none
  }

  .dashboard-banner-network-stats-label {
    font-size: 12px!important;
    white-space: normal!important;
   }

   .dashboard-data {
    white-space:normal!important
   }
}

@media screen and (min-device-width: 290px) and (max-device-width: 768px){
  .mb-sm {
    margin-bottom: 15px;
  }
 
  .pb-sm {
    padding-bottom: 15px;
  }

  .border-bottm-sm {
    border-bottom: 1px solid #dee2e6;
  }

  .border-right-sm1, .border-right-sm2 {
    border-right: 0 !important;
  }

  .dashboard-data {
    font-size: 0.7375rem!important

  }

 .dashboard-banner-network-stats-label {
    font-size: 12px!important;
    white-space: normal!important;
 }

}
@media (min-width: 768px) {

}

.footer-social a {
  display: flex;
  align-items: center;
  justify-content: center;
  float: left;
  width: 25px;
  height: 25px;
  padding: 6px;
  border-radius: 50%;
  color: #fff!important;
  background: #eeeeee2e;
  margin: 0 5px;
  text-align: center;
  transition: .3s ease-in-out;
}
.footer-social a:hover {  
  color:#333!important;
  background:#fff;
  text-decoration:none
}

a.footer-link:hover {
  color: #fff!important;
}

.card {
  box-shadow: 0 .5rem 1.2rem rgba(189,197,209,.2)!important;
}
.mobile_arrow {
  position: absolute;
  right: 6px;
}

.page-number {
  border-color: #E4E6CC!important;
}

.navbar { 
  box-shadow: 0px 0px 8px 0px rgb(21 53 80 / 12%)!important;
}

</style>

